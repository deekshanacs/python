
def find_maximum(numbers):
    max_number = numbers[0]
    for num in numbers:
        if num > max_number:
            max_number = num
    
    return max_number

numbers = [int(x) for x in input("Enter the list of numbers separated by space: ").split()]

max_element = find_maximum(numbers)

print("Maximum element in the list:", max_element)
